%ul.propositions-list
  - @jobs.each do |job|
    %li
      .proposition
        %h3 
          %a{href: job_path(job)} #{job.title}   
        - r_company = Company.all.shuffle.first 
        = link_to r_company.name, company_path(r_company)
        %span Posted: #{job.created_at.strftime("%d/%m/%y")}
    