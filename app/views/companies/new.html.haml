.form-block
  =form_for(@company)do |f|
    .form-block-fields
      =f.text_field :name, placeholder: 'Company name'  
      =f.text_field :email, placeholder: 'company@email.com' 
      =f.text_field :url, placeholder: 'Company website url' 
      =f.text_field :city, placeholder: 'City' 
      =f.text_field :country, placeholder: 'Country' 

      =f.submit 'Create'
      