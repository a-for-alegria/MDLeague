.form-block  
  %h2 New Company
  .form-lock-fields
    = form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f|
      = f.email_field :email, autofocus: true, placeholder: 'Email'
      = f.password_field :password, autocomplete: "off", placeholder: 'Password'
      = f.password_field :password_confirmation, autocomplete: "off", placeholder: 'Password confirmation'
      = f.text_field :name, placeholder: 'Company name'
      = f.text_field :url, placeholder: 'website URL'
      = f.text_field :city, placeholder: 'City'
      = f.text_field :country, placeholder: 'Country'
      = f.submit "Sign up"
  = render "devise/shared/links"